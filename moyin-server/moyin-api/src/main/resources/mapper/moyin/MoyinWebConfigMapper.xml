<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moyin.tts.mapper.MoyinWebConfigMapper">
    <update id="setUserProtocol">
        update moyin_web_config
        set user_protocal=#{protocol}
        where id = 1
    </update>

    <select id="getUserProtocol" resultType="java.lang.String">
        select user_protocal
        from moyin_web_config
        where id = 1;
    </select>
</mapper>